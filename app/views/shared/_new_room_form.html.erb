<%= form_with(model: @room, remote: true, class: "d-flex gap-1 mb-1", data: { controller: "reset-form", action: "turbo:submit-end->reset-form#reset" }) do |form| %>
  <%= form.text_field :name, class: "form-control " %>
  <%= form.submit data: {disable_with: false}, class: "btn btn-primary " %>
<% end %>